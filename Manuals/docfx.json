{
    "metadata": [
        {
            "src": [
                {
                    "src": "..",
                    "files": [
                        ".docs/__dummy.cs",
                    ],
                }
            ],
            "dest": "api"
        }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "PLATEAU-UC22-023-Pedestrian-Simulation-Result-Viewer",
            "_appFooter": "PLATEAU-UC22-023-Pedestrian-Simulation-Result-Viewer",
            "_enableSearch": true
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "api",
                "files": [
                    "*.yml"
                ],
                "dest": "api"
            },
            {
                "src": "manual",
                "files": [
                    "toc.yml",
                    "*.md"
                ],
                "dest": "manual"
            }
        ],
        "resource": [
            {
                "files": [
                    "png_files/*.png"
                ]
            }
        ],
        "sitemap": {
            "baseUrl": "https://hiroakigoto.github.io/PLATEAU-UC22-023-Pedestrian-Simulation-Result-Viewer",
            "changefreq": "weekly",
            "fileOptions": {
                "api/*": {
                    "changefreq": "daily"
                }
            }
        },
        "dest": "../.docs/_site"
    }
}