{
  "metadata": [
    {
      "src": [
        {
          "files": [
            ".docs/__dummy.cs"
          ]
        }
      ],
      "dest": "api",
    }
  ],
  "build": {
    "globalMetadata": {
            "_appTitle": "PLATEAU-UC22-023-Pedestrian-Simulation-Result-Viewer",
            "_appFooter": "PLATEAU-UC22-023-Pedestrian-Simulation-Result-Viewer",
            "_enableSearch": true
        },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
